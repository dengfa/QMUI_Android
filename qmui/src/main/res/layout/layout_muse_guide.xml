<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent">    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/trackPanel"        android:layout_width="match_parent"        android:layout_height="154dp"        android:layout_marginBottom="66dp"        android:background="#17171A"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent">        <Space            android:id="@+id/space"            android:layout_width="1dp"            android:layout_height="match_parent"            android:layout_marginEnd="100dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent" />        <ImageView            android:id="@+id/ivTrack"            android:layout_width="wrap_content"            android:layout_height="138dp"            android:layout_marginTop="16dp"            android:src="@drawable/guide_track"            app:layout_constraintStart_toStartOf="@id/space"            app:layout_constraintTop_toTopOf="parent" />        <com.airbnb.lottie.LottieAnimationView            android:id="@+id/lottieCutTips"            android:layout_width="400dp"            android:layout_height="130dp"            android:layout_marginTop="10dp"            android:visibility="gone"            app:layout_constraintStart_toStartOf="@id/ivTrack"            app:layout_constraintTop_toTopOf="@id/ivTrack"            app:lottie_autoPlay="true"            app:lottie_fileName="guide/guide_scrolling.json"            app:lottie_loop="true" />    </androidx.constraintlayout.widget.ConstraintLayout>    <com.qmuiteam.qmui.widget.guide.GuideCoverView        android:id="@+id/guideCover"        android:layout_width="match_parent"        android:layout_height="match_parent" />    <com.qmuiteam.qmui.widget.guide.TipsView        android:id="@+id/splitTips"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="20dp"        android:layout_marginBottom="57dp"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent" />    <com.qmuiteam.qmui.widget.guide.TipsView        android:id="@+id/cutTips"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginBottom="192dp"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent" />    <com.qmuiteam.qmui.widget.guide.TipsView        android:id="@+id/reOrderTips"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="150dp"        android:layout_marginBottom="172dp"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent" />    <com.qmuiteam.qmui.widget.guide.TipsView        android:id="@+id/tagTips"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="200dp"        android:layout_marginBottom="180dp"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent" /></androidx.constraintlayout.widget.ConstraintLayout>